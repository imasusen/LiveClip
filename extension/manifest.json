{
  "manifest_version": 2,
  "name": "LiveClipExtension",
  "description": "Clipping anywhere from Chrome Tab.",
  "version": "0.1",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Clipping Anyware"
  }
}
